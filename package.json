    "express": "^4.22.1",
    "express-validator": "^7.3.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.3",
    "moment": "^2.30.1",
    "mongoose": "^7.8.8",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.14.0",
    "sharp": "^0.33.5",
    "tesseract.js": "^5.1.1",
    "winston": "^3.19.0"
  },
