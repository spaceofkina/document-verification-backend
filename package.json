{
  "name": "brgy-lajong-document-request-system",
  "version": "1.0.0",
  "description": "Document Request System for Brgy Lajong, Bulan, Sorsogon with CNN and OCR verification",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "node setup.js",
    "setup:reset": "node setup.js --reset",
